@{
    ViewData["Title"] = "Home Page";
}



@model List<Customer>

<table class="table table-bordered">
    <thead class="fw-bold">
        <tr>
            <td>customer_Id</td>
            <td>product_Id</td>
            <td>qty</td>
            <td>rating</td>

        </tr
    </thead>

    @foreach (var x in Model)
    {
        <tbody>
            <tr>
                <td>@x.customer_ID</td>
@*                 <td>@x.product_ID</td>
                <td>@x.qty</td>
                <td>@x.rating</td> *@
            </tr>
        </tbody>
    }